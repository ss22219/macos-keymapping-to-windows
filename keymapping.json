{
  "title": "MacOS",
  "rules": [
    {
      "description": "Spotlight",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "modifiers": {
              "mandatory": [
                "left_control"
              ]
            },
            "key_code": "s"
          },
          "to": [
            {
              "repeat": false,
              "key_code": "spacebar",
              "modifiers": [
                "left_gui"
              ]
            }
          ]
        }
      ]
    },
    {
      "description": "Switch Keyboard",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "modifiers": {
              "mandatory": [
                "left_gui"
              ]
            },
            "key_code": "spacebar"
          },
          "to": [
            {
              "repeat": false,
              "key_code": "spacebar",
              "modifiers": [
                "left_control"
              ]
            }
          ]
        }
      ]
    },
    {
      "description": "Screenshot",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "modifiers": {
              "mandatory": [
                "left_gui",
                "left_alt"
              ]
            },
            "key_code": "a"
          },
          "to": [
            {
              "repeat": false,
              "key_code": "5",
              "modifiers": [
                "left_gui",
                "left_shift"
              ]
            }
          ]
        }
      ]
    },
    {
      "description": "SwitchWindow",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "modifiers": {
              "mandatory": [
                "left_alt"
              ]
            },
            "key_code": "tab"
          },
          "to": [
            {
              "repeat": false,
              "key_code": "tab",
              "modifiers": [
                "left_gui"
              ]
            }
          ]
        }
      ]
    },
    {
      "description": "SwitchTab",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "modifiers": {
              "mandatory": [
                "left_gui"
              ]
            },
            "key_code": "tab"
          },
          "to": [
            {
              "repeat": false,
              "key_code": "tab",
              "modifiers": [
                "left_control"
              ]
            }
          ]
        }
      ]
    },
    {
      "description": "QuitApp",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "modifiers": {
              "mandatory": [
                "left_alt"
              ]
            },
            "key_code": "f4"
          },
          "to": [
            {
              "repeat": false,
              "key_code": "q",
              "modifiers": [
                "left_gui"
              ]
            }
          ]
        }
      ]
    },
    {
      "description": "GoToStart",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "home",
            "modifiers": {
              "mandatory": [
                "left_gui"
              ]
            }
          },
          "to": [
            {
              "repeat": false,
              "key_code": "up_arrow",
              "modifiers": [
                "left_gui"
              ]
            }
          ]
        }
      ]
    },
    {
      "description": "GoToEnd",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "end",
            "modifiers": {
              "mandatory": [
                "left_gui"
              ]
            }
          },
          "to": [
            {
              "repeat": false,
              "key_code": "down_arrow",
              "modifiers": [
                "left_gui"
              ]
            }
          ]
        }
      ]
    },
    {
      "description": "GoToLineStart",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "home"
          },
          "to": [
            {
              "repeat": false,
              "key_code": "left_arrow",
              "modifiers": [
                "left_gui"
              ]
            }
          ]
        }
      ]
    },
    {
      "description": "GoToLineEnd",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "end"
          },
          "to": [
            {
              "repeat": false,
              "key_code": "right_arrow",
              "modifiers": [
                "left_gui"
              ]
            }
          ]
        }
      ]
    },
    {
      "description": "PreviousWord",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "left_arrow",
            "modifiers": {
              "mandatory": [
                "left_gui"
              ]
            }
          },
          "to": [
            {
              "repeat": false,
              "key_code": "left_arrow",
              "modifiers": [
                "left_alt"
              ]
            }
          ]
        }
      ]
    },
    {
      "description": "NextWord",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "right_arrow",
            "modifiers": {
              "mandatory": [
                "left_gui"
              ]
            }
          },
          "to": [
            {
              "repeat": false,
              "key_code": "right_arrow",
              "modifiers": [
                "left_alt"
              ]
            }
          ]
        }
      ]
    },
    {
      "description": "DeleteWord",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "delete_or_backspace",
            "modifiers": {
              "mandatory": [
                "left_gui"
              ]
            }
          },
          "to": [
            {
              "repeat": false,
              "key_code": "delete_or_backspace",
              "modifiers": [
                "left_alt"
              ]
            }
          ],
          "conditions": [
            {
              "type": "frontmost_application_unless",
              "bundle_identifiers": [
                "com.apple.finder"
              ]
            }
          ]
        }
      ]
    },
    {
      "description": "TaskManager",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "delete_forward",
            "modifiers": {
              "mandatory": [
                "left_gui",
                "left_alt"
              ]
            }
          },
          "to": [
            {
              "shell_command": "open '/System/Applications/Utilities/Activity Monitor.app/'"
            }
          ]
        }
      ]
    },
    {
      "description": "RiderReplace",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "H",
            "modifiers": {
              "mandatory": [
                "left_gui"
              ]
            }
          },
          "to": [
            {
              "repeat": false,
              "key_code": "h",
              "modifiers": [
                "left_control"
              ]
            }
          ],
          "conditions": [
            {
              "type": "frontmost_application_if",
              "bundle_identifiers": [
                "com.jetbrains.rider"
              ]
            }
          ]
        }
      ]
    }
  ]
}
